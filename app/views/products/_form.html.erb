<%= render('layouts/errors', :object => @product) %>

  <%= form_for [@brand, @product] do |f| %>
    <%= f.label :item %>
    <%= f.text_field :item %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :price %>
    <%= f.number_field :price %>

    <%= f.hidden_field :user_id, value: current_user.id%>
  <br>
  <p>  <%= f.file_field :picture %></p>
    <p><%= f.submit class: 'btn btn-success' %></p>
  <% end %>
