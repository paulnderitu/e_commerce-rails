<%= render('layouts/errors', :object => @product) %>

  <%= form_for [@brand, @product] do |f| %>
    <%= f.label :item %>
    <%= f.text_field :item %>
    <%= f.label :price %>
    <%= f.number_field :price %>

    <%= f.select(:user_id) do %>
    <% User.all.each do |usr| %>
      <%= content_tag(:option, usr.email, value: usr.id) %>
    <% end %>
  <% end %>
  <br>
  <p>  <%= f.file_field :picture %></p>
    <p><%= f.submit %></p>
  <% end %>
