<h1><a href="<%= @product.item %>"></a></h1>
<%= image_tag @product.picture_url.to_s, class: 'project_image'%>

<h3>Upvote or Downvote this product:</h3>
<%= link_to like_brand_product_path(@product), method: :put do %>
  Upvote
  <%= @product.get_upvotes.size %>
<% end %>

<%= link_to dislike_brand_product_path(@product), method: :put do %>
  Downvote
  <%= @product.get_downvotes.size %>
<% end %>
<%= @product.score%>
