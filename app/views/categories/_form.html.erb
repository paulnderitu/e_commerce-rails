<%= render "layouts/errors", :object => @category %>

  <%= form_for @category do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    Select Companies:
  

    <% @brand.each do |brand| %>
<%= f.label brand.title%>
      <%= check_box_tag "brand_ids[]", brand.id, false, class: 'checkbox-inline' %>
    <% end %>

    <%= f.submit %>
  <% end %>
