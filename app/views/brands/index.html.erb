<h1>Brands</h1>

<h1>Welcome to E Spares</h1>

<%= image_tag "spaee.jpeg" %>

  <ul>
    <% @brands.each do |brand| %>
    <li><%= link_to brand.title, brand_path(brand) %></li>
    <% end %>
  </ul>

  <hr>


  <% if user_signed_in? %>
    <% if current_user.admin? %>
      <hr>
       <%= link_to "New Brand", new_brand_path %>
    <% end %>
  <% end %>
